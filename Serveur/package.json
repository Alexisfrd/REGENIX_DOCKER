{
  "name": "sample-infra-v4",
  "version": "4.0.0",
  "description": "Support project for IoT platform course (ESEO)",
  "scripts": {

    "backend": "node bin/backend.js",
    "client": "node bin/client.js -n regenix -l eseo -p pass",
    "router": "node bin/router.js",
    "publishToRedis-DO": "node bin/publishToRedis.js -q 0 -a 21 -i 14 -n test999 -d 24-07-2029-09-27-43 -f 1 -c 1",
    "publishToRedis-PH": "node bin/publishToRedis.js -q 1 -a 7 -i 4  -n test999 -d 24-07-2029-09-27-43 -f 1 -c 1",
    "publishToRedis-TEMP": "node bin/publishToRedis.js -q 2 -a 38 -i 34 -n test999 -d 24-0-2029-09-27-43 -f 1 -c 1",
    "publishToRedis-Debit": "node bin/publishToRedis.js -q 3 -a 1 -i 2 -n test999 -d 24-0-2029-09-27-43 -f 1 -c 1",
    "rest-temp": "node bin/rest_temp.js",
    "writeToCsv-PH": "node bin/writeToCsv.js -s ph",
    "writeToCsv-DO": "node bin/writeToCsv.js -s do",
    "writeToCsv-TEMP": "node bin/writeToCsv.js -s temp",
    "writeToCsv-DEBIT": "node bin/writeToCsv.js -s debit",

    "start-all": "pm2 start ecosystem.config.js",
    "stop-all": "pm2 stop ecosystem.config.js",
    "start-test": "pm2 start testEcosystem.config.js",
    "testW-Temp": "pm2 start testWTemp.config.js",

    "client-good-login-but-bad-target": "node bin/client.js -l test -p pass -t 10",
    "client-bad-login": "node bin/client.js -l test -p badpass -t 1",
    "curl": "curl -d '{\"username\":\"my_username\", \"email\":\"my_email\"}' -H \"Content-Type: application/json\" -X POST http://localhost:3000/data",
    "curl-good-login": "curl -d '{\"login\":\"test\", \"password\":\"pass\"}' -H \"Content-Type: application/json\" -X POST http://localhost:3000/login",
    "curl-bad-login": "curl -d '{\"login\":\"test\", \"password\":\"badpassword\"}' -H \"Content-Type: application/json\" -X POST http://localhost:3000/login",
    "rabbitmq-docker": "docker run -it --rm --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:3-management",
    "rabbitmq-receiver": "node bin/rabbitmq-receiver.js",
    "rabbitmq-sender": "node bin/rabbitmq-sender.js",
    "redis-docker": "docker run -it --rm --name redis -p 6379:6379 redis",
    "redis-test-sync": "node bin/redis.js",
    "redis-test-async": "node bin/redis.js -a",
    "check-redis": "node bin/check_redis.js -c angers"
  },
  "author": "Jean-Samuel Reynaud <jeansamuel.reynaud@qowisio.com>",
  "license": "GPLv3",
  "dependencies": {
    "amqplib": "^0.10.0",
    "argparse": "^2.0.1",
    "axios": "^1.7.2",
    "chart.js": "^4.4.3",
    "chartjs-plugin-annotation": "^3.0.1",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "ioredis": "^5.4.1",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "ps-node": "^0.1.6",
    "redis": "^4.7.0",
    "serialport": "^12.0.0",
    "tree-kill": "^1.2.2"
  }
}
